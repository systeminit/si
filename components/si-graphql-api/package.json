{
  "name": "si-graphql-api",
  "private": true,
  "version": "0.1.0",
  "description": "System Initiative GraphQL API",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "webpack",
    "watch": "webpack --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js && npm run bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "start": "npm run watch",
    "prod": "node --enable-source-maps dist/app.bundle.js",
    "test": "npm run build:js && jest --passWithNoTests --detectOpenHandles --forceExit",
    "test-watch": "npm run build:js && jest --passWithNoTests --detectOpenHandles --forceExit --watch"
  },
  "keywords": [],
  "author": "The System Initiative, Inc.",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/jest": "^25.2.2",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/lodash": "^4.14.150",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-graphql": "^3.1.1",
    "eslint-plugin-prettier": "^3.1.3",
    "file-loader": "^6.0.0",
    "grpc-tools": "^1.8.1",
    "jest": "^26.0.1",
    "nodemon": "^2.0.3",
    "nodemon-webpack-plugin": "^4.3.1",
    "prettier": "^2.0.5",
    "ts-protoc-gen": "^0.12.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.6",
    "@grpc/proto-loader": "^0.5.4",
    "@iarna/toml": "^2.2.5",
    "@opentelemetry/core": "^0.8.0",
    "@opentelemetry/exporter-collector": "^0.8.0",
    "@opentelemetry/exporter-jaeger": "^0.8.0",
