    "@opentelemetry/plugin-grpc": "^0.8.0",
    "@opentelemetry/plugin-http": "^0.8.0",
    "@opentelemetry/plugin-https": "^0.8.1",
    "@opentelemetry/tracing": "^0.8.0",
    "apollo-datasource": "^0.7.0",
    "apollo-server": "^2.13.1",
    "change-case": "^4.1.1",
    "dotenv": "^8.2.0",
    "graphql": "^15.0.0",
    "graphql-subscriptions": "^1.1.0",
    "grpc": "^1.24.2",
    "grpc-caller": "^0.13.0",
    "iterall": "^1.3.0",
