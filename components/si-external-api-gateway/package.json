{
  "name": "si-external-api-gateway",
  "private": true,
  "version": "0.1.0",
  "description": "System Initiative External API Gateway",
  "main": "dist/index.js",
  "scripts": {
    "bundle": "webpack",
    "watch": "webpack --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js && npm run bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".js,.ts,.tsx\" --ignore \"src/**/*.d.ts\" --source-maps inline",
    "start": "npm run watch",
    "prod": "node --enable-source-maps dist/app.bundle.js",
    "test": "npm run build:js && jest --passWithNoTests --detectOpenHandles --forceExit",
    "test-watch": "npm run build:js && jest --passWithNoTests --detectOpenHandles --forceExit --watch"
  },
  "keywords": [],
  "author": "The System Initiative, Inc.",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/google-protobuf": "^3.7.2",
    "@types/jest": "^24.0.19",
    "@types/node": "^13.7.2",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^6.5.1",
    "eslint-config-prettier": "^6.4.0",
    "eslint-plugin-graphql": "^3.1.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^6.0.0",
    "grpc-tools": "^1.8.0",
    "jest": "^25.1.0",
    "nodemon-webpack-plugin": "^4.3.1",
    "prettier": "^1.18.2",
    "ts-protoc-gen": "^0.12.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.6.4",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "@aws-sdk/client-ec2": "^1.0.0-alpha.2",
    "@aws-sdk/client-eks": "^1.0.0-alpha.1",
    "@grpc/proto-loader": "^0.5.2",
    "@iarna/toml": "^2.2.3",
    "@usefultools/monads": "^3.0.3",
    "aws-sdk": "^2.621.0",
    "dotenv": "^8.2.0",
    "grpc": "^1.24.2",
    "grpc-caller": "^0.13.0",
    "protobufjs": "^6.8.8",
    "winston": "^3.2.1"
  }
}
