    "@babel/preset-env": "^7.9.6",
    "@babel/preset-typescript": "^7.9.0",
    "@types/express": "^4.17.8",
    "@types/express-ws": "^3.0.0",
    "@types/jest": "^24.0.19",
    "@types/morgan": "^1.9.1",
    "@types/node": "^13.11.1",
    "@typescript-eslint/eslint-plugin": "^2.5.0",
    "@typescript-eslint/parser": "^2.5.0",
