{
  "name": "si-registry",
  "private": true,
  "version": "0.1.0",
  "description": "Component/Entity Registry",
  "main": "lib/index.js",
  "scripts": {
    "bundle": "webpack --mode production",
    "watch": "webpack --watch",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "build": "npm run build:types && npm run build:js && npm run bundle",
    "build:types": "tsc --emitDeclarationOnly",
    "build:js": "babel src --out-dir lib --extensions \".ts,.tsx\" --source-maps inline",
    "start": "npm run watch",
    "test": "jest --passWithNoTests --detectOpenHandles --forceExit",
    "test-watch": "jest --passWithNoTests --detectOpenHandles --forceExit --watch"
  },
  "author": "The System Initiative, Inc.",
  "license": "UNLICENSED",
