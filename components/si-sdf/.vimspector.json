{
  "$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
  "configurations": {
    "Launch si-sdf-server": {
      "adapter": "CodeLLDB",
      "configuration": {
        "request": "launch",
        "program": "${workspaceRoot}/../../target/debug/si-sdf-server",
        "sourceLanguages": ["rust"]
      }
    },
    "Attach to running si-sdf-server": {
      "adapter": "CodeLLDB",
      "configuration": {
        "request": "attach",
        "program": "${workspaceRoot}/../../target/debug/si-sdf-server",
        "sourceLanguages": ["rust"]
      }
    }
  }
}
