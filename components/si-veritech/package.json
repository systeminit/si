{
  "name": "si-veritech",
  "private": "true",
  "version": "0.1.0",
  "description": "Si Attribute Editing",
  "main": "src/index.ts",
  "scripts": {
    "clean": "rm -rf ./dist",
    "build": "tsc",
    "build:watch": "npm run clean && tsc --watch",
    "watch": "ts-node-dev --respawn --transpile-only --rs --exit-child ./src/index.ts",
    "start": "node ./dist/index.js",
    "start:debug": "npm run build && node --inspect-brk ./dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch --forceExit",
    "test:debug": "node --inspect-brk node_modules/.bin/jest --runInBand",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "author": "System Initiative, Inc.",
  "license": "Proprietary",
  "devDependencies": {
    "@types/debug": "^4.1.5",
    "@types/express": "^4.17.11",
    "@types/express-ws": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/json-schema": "^7.0.7",
    "@types/lodash": "^4.14.168",
    "@types/morgan": "^1.9.2",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.10",
    "@types/validator": "^13.1.3",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-jest": "^24.1.9",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.3",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "debug": "^4.3.1",
    "express": "^4.17.1",
    "express-ws": "^4.0.0",
    "lodash": "^4.17.21",
    "morgan": "^1.10.0",
    "si-entity": "../si-entity",
    "si-registry": "../si-registry"
  }
}
