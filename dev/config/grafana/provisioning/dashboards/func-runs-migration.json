        "defaults": {
          "unit": "percent",
          "min": 0,
          "max": 100,
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 50,
                "color": "yellow"
              },
              {
                "value": 90,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "showThresholdLabels": false,
        "showThresholdMarkers": true
