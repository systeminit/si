{
  "id": null,
  "uid": "bifrost-websocket-metrics",
  "title": "Bifrost WebSocket Performance Metrics",
  "tags": [
    "sdf",
    "bifrost",
    "websocket",
    "nats",
    "performance"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 0,
  "refresh": "5s",
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Data Source",
        "query": "prometheus",
        "refresh": 1,
        "hide": 0
      },
      {
        "name": "service",
        "type": "query",
        "label": "Service",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(sdf_bifrost_connections_opened_total, exported_job)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "panels": [
    {
      "id": 100,
      "type": "row",
      "title": "Overview & Health Summary",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "id": 101,
      "type": "stat",
      "title": "Active Connections",
      "gridPos": {
        "x": 0,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(sdf_bifrost_active_connections{exported_job=~\"${service}\"})",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 5,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto"
      }
    },
    {
      "id": 102,
      "type": "stat",
      "title": "NATS Messages/sec",
      "gridPos": {
        "x": 6,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(rate(sdf_bifrost_select_branch_total{exported_job=~\"${service}\", branch=\"nats_message\"}[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 0.1,
                "color": "yellow"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto"
      }
    },
    {
      "id": 103,
      "type": "stat",
      "title": "End-to-End Latency p95",
      "gridPos": {
        "x": 12,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "histogram_quantile(0.95, sum(rate(sdf_bifrost_nats_to_ws_latency_ms_bucket{exported_job=~\"${service}\"}[5m])) by (le))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "green"
              },
              {
                "value": 100,
                "color": "yellow"
              },
              {
                "value": 500,
                "color": "red"
              }
            ]
          }
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto"
      }
    },
    {
      "id": 104,
      "type": "stat",
      "title": "WS Send Success Rate",
      "gridPos": {
        "x": 18,
        "y": 1,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(rate(sdf_bifrost_ws_send_latency_ms_count{exported_job=~\"${service}\", result=\"success\"}[5m])) / sum(rate(sdf_bifrost_ws_send_latency_ms_count{exported_job=~\"${service}\"}[5m]))",
          "refId": "A"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": 0,
                "color": "red"
              },
              {
                "value": 0.95,
                "color": "yellow"
              },
              {
                "value": 0.99,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "textMode": "value_and_name",
        "colorMode": "background",
        "graphMode": "area",
        "orientation": "auto"
      }
    },
    {
      "id": 200,
      "type": "row",
      "title": "End-to-End Latency",
      "gridPos": {
        "x": 0,
        "y": 5,
        "w": 24,
        "h": 1
      },
      "collapsed": false
    },
    {
      "id": 201,
      "type": "timeseries",
      "title": "End-to-End Latency (NATS Receipt â†’ WebSocket Send)",
      "description": "Total time from receiving NATS message to completing WebSocket send. This is the primary diagnostic metric for reported delays.",
      "interval": "1m",
      "gridPos": {
        "x": 0,
        "y": 6,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "histogram_quantile(0.50, sum(rate(sdf_bifrost_nats_to_ws_latency_ms_bucket{exported_job=~\"${service}\"}[5m])) by (le))",
          "refId": "A",
          "legendFormat": "p50"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "histogram_quantile(0.95, sum(rate(sdf_bifrost_nats_to_ws_latency_ms_bucket{exported_job=~\"${service}\"}[5m])) by (le))",
          "refId": "B",
          "legendFormat": "p95"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "histogram_quantile(0.99, sum(rate(sdf_bifrost_nats_to_ws_latency_ms_bucket{exported_job=~\"${service}\"}[5m])) by (le))",
          "refId": "C",
          "legendFormat": "p99"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "histogram_quantile(1.0, sum(rate(sdf_bifrost_nats_to_ws_latency_ms_bucket{exported_job=~\"${service}\"}[5m])) by (le))",
          "refId": "D",
          "legendFormat": "max"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "linear",
            "lineWidth": 1,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "p50"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "p95"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "yellow"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "p99"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "orange"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "max"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "red"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "legend": {
          "displayMode": "list",
          "placement": "bottom",
          "showLegend": true,
          "calcs": [
            "last"
          ]
        },
        "tooltip": {
          "mode": "multi",
          "sort": "none"
        }
      }
    },
    {
