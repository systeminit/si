  "scripts": {
    "clean": "rm -rf ./dist ./lib ./target",
    "build": "vite build",
    "build:check": "vue-tsc --noEmit",
    "build:clean": "npm run clean && npm run build",
    "build:watch": "npm run clean && tsc --watch",
    "build:pedantically": "npm run build:check && npm run build",
    "watch": "npm run build:watch",
    "lint": "eslint src --ext .ts,.js,.vue --ignore-path .gitignore",
    "lint:fix": "npm run lint -- --fix",
    "lint:strict": "STRICT_LINT=1 npm run lint",
    "lint:summary": "npm run lint -- --format summary-chart",
    "fmt": "npm run lint:fix",
    "fmt:check": "npm run lint",
    "check": "npm run fmt:check && npm run build:check",
    "dev": "npm run start",
    "start": "vite",
    "serve": "vite preview --port 8080",
    "test": "echo 'Sorry, no tests yet!'",
