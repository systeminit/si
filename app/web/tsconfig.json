{
  "extends": "@si/tsconfig/tsconfig.browser.json",
  "compilerOptions": {
    "noUncheckedIndexedAccess": true,
    "baseUrl": "./",
    "paths": {
      "@/*": [
        "src/*",
        "app/web/src/*",
      ]
    },
    "declaration": false,
    "declarationMap": false,
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.d.ts",
    "src/**/*.tsx",
    "src/**/*.vue",
    "cypress.d.ts",
  ],
}
