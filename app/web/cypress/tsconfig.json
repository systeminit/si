{
  "compilerOptions": {
    "lib": ["es2015", "dom"],
    "types": ["cypress"],
    "isolatedModules": false,
    "allowJs": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "target": "es5",
  },
  "include": ["**/*.ts", "../src/**/*.ts"]
}
