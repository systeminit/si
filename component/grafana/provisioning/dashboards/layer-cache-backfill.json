{
  "id": null,
  "uid": "layer-cache-backfill",
  "title": "Layer Cache Backfill",
  "tags": [
    "layercache",
    "backfill",
    "s3",
    "migration"
  ],
  "timezone": "browser",
  "schemaVersion": 39,
  "version": 0,
  "refresh": "10s",
  "annotations": {
    "list": [
      {
        "builtIn": 1,
        "datasource": {
          "type": "grafana",
          "uid": "-- Grafana --"
        },
        "enable": true,
        "hide": true,
        "iconColor": "rgba(0, 211, 255, 1)",
        "name": "Annotations & Alerts",
        "type": "dashboard"
      }
    ]
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "label": "Data Source",
        "query": "prometheus",
        "refresh": 1,
        "hide": 0
      },
      {
        "name": "service",
        "type": "query",
        "label": "Service",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(layer_cache_backfill_items_processed_total, exported_job)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      },
      {
        "name": "cache_type",
        "type": "query",
        "label": "Cache Type",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "query": "label_values(layer_cache_backfill_items_processed_total, cache_type)",
        "refresh": 2,
        "sort": 1,
        "multi": true,
        "includeAll": true
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "stat",
      "title": "Total Items Processed",
      "interval": "1m",
      "gridPos": {
        "x": 0,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(layer_cache_backfill_items_processed_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"})",
          "refId": "A",
          "description": "Total number of items processed across all selected caches"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "blue"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 2,
      "type": "stat",
      "title": "Total Items Uploaded",
      "interval": "1m",
      "gridPos": {
        "x": 6,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(layer_cache_backfill_items_uploaded_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"})",
          "refId": "A",
          "description": "Total number of items uploaded to S3"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 3,
      "type": "stat",
      "title": "Total Items Skipped",
      "interval": "1m",
      "gridPos": {
        "x": 12,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(layer_cache_backfill_items_skipped_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"})",
          "refId": "A",
          "description": "Total number of items skipped (already in S3)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "yellow"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 4,
      "type": "stat",
      "title": "Total Bytes Uploaded",
      "interval": "1m",
      "gridPos": {
        "x": 18,
        "y": 0,
        "w": 6,
        "h": 4
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(layer_cache_backfill_bytes_uploaded_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"})",
          "refId": "A",
          "description": "Total bytes uploaded to S3"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "purple"
              }
            ]
          }
        }
      },
      "options": {
        "graphMode": "area",
        "colorMode": "value",
        "orientation": "auto",
        "textMode": "value_and_name",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "Processing Rate by Cache Type",
      "interval": "1m",
      "gridPos": {
        "x": 0,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(rate(layer_cache_backfill_items_processed_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"}[$__interval])) by (cache_type)",
          "legendFormat": "{{cache_type}}",
          "refId": "A",
          "description": "Rate of items processed per second by cache type"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull",
            "max",
            "mean"
          ]
        }
      }
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Upload vs Skip Rate by Cache Type",
      "interval": "1m",
      "gridPos": {
        "x": 12,
        "y": 4,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(rate(layer_cache_backfill_items_uploaded_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"}[$__interval])) by (cache_type)",
          "legendFormat": "{{cache_type}} - uploaded",
          "refId": "A",
          "description": "Rate of items uploaded to S3"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(rate(layer_cache_backfill_items_skipped_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"}[$__interval])) by (cache_type)",
          "legendFormat": "{{cache_type}} - skipped",
          "refId": "B",
          "description": "Rate of items skipped (already in S3)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull",
            "max",
            "mean"
          ]
        }
      }
    },
    {
      "id": 7,
      "type": "timeseries",
      "title": "Upload Throughput by Cache Type",
      "interval": "1m",
      "gridPos": {
        "x": 0,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(rate(layer_cache_backfill_bytes_uploaded_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"}[$__interval])) by (cache_type)",
          "legendFormat": "{{cache_type}}",
          "refId": "A",
          "description": "Rate of bytes uploaded to S3 per second"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "Bps",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        }
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull",
            "max",
            "mean"
          ]
        }
      }
    },
    {
      "id": 8,
      "type": "piechart",
      "title": "Items Distribution by Cache Type",
      "interval": "1m",
      "gridPos": {
        "x": 12,
        "y": 12,
        "w": 12,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "sum(layer_cache_backfill_items_processed_total{exported_job=~\"${service}\", cache_type=~\"${cache_type}\"}) by (cache_type)",
          "legendFormat": "{{cache_type}}",
          "refId": "A",
          "description": "Distribution of processed items across cache types"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short"
        }
      },
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "lastNotNull"
          ],
          "values": [
            "value",
            "percent"
          ]
        },
        "pieType": "pie",
        "tooltip": {
          "mode": "single"
        }
      }
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "${cache_type} - Item Count Progress",
      "repeat": "cache_type",
      "repeatDirection": "v",
      "interval": "1m",
      "gridPos": {
        "x": 0,
        "y": 20,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "layer_cache_backfill_items_processed_total{exported_job=~\"${service}\", cache_type=\"${cache_type}\"}",
          "legendFormat": "processed",
          "refId": "A",
          "description": "Total items processed for this cache type"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "layer_cache_backfill_items_uploaded_total{exported_job=~\"${service}\", cache_type=\"${cache_type}\"}",
          "legendFormat": "uploaded",
          "refId": "B",
          "description": "Total items uploaded to S3"
        },
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "layer_cache_backfill_items_skipped_total{exported_job=~\"${service}\", cache_type=\"${cache_type}\"}",
          "legendFormat": "skipped",
          "refId": "C",
          "description": "Total items skipped (already in S3)"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "processed"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "blue"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "uploaded"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "green"
                }
              }
            ]
          },
          {
            "matcher": {
              "id": "byName",
              "options": "skipped"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "yellow"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull",
            "max",
            "mean"
          ]
        }
      }
    },
    {
      "id": 10,
      "type": "timeseries",
      "title": "${cache_type} - Bytes Uploaded Progress",
      "repeat": "cache_type",
      "repeatDirection": "v",
      "interval": "1m",
      "gridPos": {
        "x": 0,
        "y": 28,
        "w": 24,
        "h": 8
      },
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "targets": [
        {
          "datasource": {
            "type": "prometheus",
            "uid": "${datasource}"
          },
          "expr": "layer_cache_backfill_bytes_uploaded_total{exported_job=~\"${service}\", cache_type=\"${cache_type}\"}",
          "legendFormat": "bytes uploaded",
          "refId": "A",
          "description": "Total bytes uploaded to S3 for this cache type"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "bytes",
          "color": {
            "mode": "palette-classic"
          },
          "custom": {
            "lineWidth": 2,
            "fillOpacity": 10,
            "showPoints": "never"
          }
        },
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "bytes uploaded"
            },
            "properties": [
              {
                "id": "color",
                "value": {
                  "mode": "fixed",
                  "fixedColor": "purple"
                }
              }
            ]
          }
        ]
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": [
            "lastNotNull",
            "max",
            "mean"
          ]
        }
      }
    }
  ],
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "timepicker": {
    "refresh_intervals": [
      "5s",
      "10s",
      "30s",
      "1m",
      "5m",
      "15m",
      "30m",
      "1h",
      "2h",
      "1d"
    ]
  }
}
