{
  "name": "system-initiative-api-client",
  "version": "0.0.1",
  "description": "TypeScript/JavaScript SDK for the System Initiative Public API",
  "author": "System Initiative <technical-operations@systeminit.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/systeminit/si"
  },
  "homepage": "https://systeminit.com",
  "bugs": {
    "url": "https://github.com/systeminit/si/issues"
  },
  "keywords": [
    "axios",
