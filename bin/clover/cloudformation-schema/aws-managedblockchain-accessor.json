  "definitions": {
    "AccessorStatus": {
      "type": "string",
      "enum": ["AVAILABLE", "PENDING_DELETION", "DELETED"]
    },
    "AccessorType": {
      "type": "string",
      "enum": ["BILLING_TOKEN"]
