      "type": "string",
      "maxLength": 200,
      "minLength": 1,
      "pattern": "^arn:(aws|aws-cn|aws-us-gov):[a-zA-Z0-9-]+:[a-zA-Z0-9-]*:\\d{12}:([\\w+=,.@-]+[/:])*[\\w+=,.@-]+$",
      "description": "<p>A customer provided encryption key that Proton uses to encrypt data.</p>"
    },
    "Name": {
      "type": "string",
      "maxLength": 100,
      "minLength": 1,
      "pattern": "^[0-9A-Za-z]+[0-9A-Za-z_\\-]*$"
    },
